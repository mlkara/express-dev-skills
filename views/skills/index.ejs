<%- include('../partials/header') %>

<h3>To-Dos as of <%= time %></h3>

<ul>
  <% skills.forEach(function(t) { %>
    <li>
      <a href="/skills/<%= t.id %>"><%= t.skill %></a>
      - 
      <%= t.done ? 'done' : 'not done' %>
    </li>
  <% }); %>
</ul>
<a href="/skills/new">Add To-Do</a>

  
<%- include('../partials/footer') %>
